        "eslint": "^7.26.0",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": "^14.17.0 || ^16",
        "npm": "^7.13.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
