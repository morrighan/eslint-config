{
    "name": "@cichol/eslint-config",
    "version": "3.0.3",
    "description": "An universal ESLint configuration for personal projects.",
    "homepage": "https://github.com/morrighan/eslint-config",
    "bugs": {
        "url": "https://github.com/morrighan/eslint-config/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/morrighan/eslint-config.git"
    },
    "scripts": {
        "test": "npm run test:eslint && npm run test:tsc && npm run test:mocha",
        "test:eslint": "npx eslint . --ext .js,.ts",
        "test:tsc": "npx tsc --project . --noEmit",
        "test:mocha": "npx mocha"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.23.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/register": "^7.13.16",
        "@types/babel__core": "^7.1.14",
        "@types/chai": "^4.2.17",
        "@types/eslint": "^7.2.12",
        "@types/lodash": "^4.14.170",
